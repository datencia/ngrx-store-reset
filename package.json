  "homepage": "https://github.com/datencia/ngrx-store-reset#readme",
  "peerDependencies": {
    "@ngrx/store": ">=5.0.0"
  },
  "devDependencies": {
    "@angular/core": "^5.2.11",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^7.5.0",
    "@ngrx/store": "^5.2.0",
    "husky": "^2.2.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
