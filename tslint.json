{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "deprecation": {
            "severity": "warn"
        },
        "quotemark": [
            true,
            "single"
        ],
        "ordered-imports": [
            false
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-empty-interface": false,
        "no-empty": false,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "no-unused-expression": false,
        "variable-name": [
            false
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock"
        ]
    },
    "rulesDirectory": []
}